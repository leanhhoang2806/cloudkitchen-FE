{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "plugins": ["react", "react-hooks"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    // Add any additional rules or overrides here
  },
  "globals": {
    "window": "readonly",
    "document": "readonly",
    "console": "readonly",
    "FormData": "readonly",
    "URL": "readonly"
    
    // Add more global variables as needed
  }
}
